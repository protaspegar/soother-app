{
  "expo": {
    "name": "soother-app",
    "description": "A simple but effective soother for your baby",
    "slug": "soother-app",
    "privacy": "unlisted",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
